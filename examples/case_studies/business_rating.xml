<doc xmlns="https://instawork.com/hyperview"
     xmlns:intercom="https://instawork.com/hyperview-intercom">&gt; 
<screen>
  <styles>
    <style alignItems="center"
           backgroundColor="white"
           flexDirection="row"
           height="72"
           id="Header"
           paddingLeft="24"
           paddingRight="24"
           paddingTop="24" />
    <style height="19"
           width="24"
           id="Header__Back"
           marginRight="16" />
    <style color="black"
           fontFamily="HKGrotesk-SemiBold"
           fontSize="24"
           id="Header__Title" />
    <style backgroundColor="white"
           flex="1"
           id="Body" />
    <style backgroundColor="white"
           flex="1"
           id="Main" />
    <style id="Section"
           borderBottomColor="#E1E3E3"
           borderBottomWidth="1"
           marginTop="24"
           marginBottom="24" />
    <style id="Section__Top"
           flexDirection="row"
           justifyContent="space-between" />
    <style id="Section__Header"
           fontFamily="HKGrotesk-Bold"
           fontSize="20"
           lineHeight="24"
           marginLeft="24"
           marginRight="24"
           marginBottom="16" />
    <style id="Section__Action"
           fontFamily="HKGrotesk-SemiBold"
           color="#4778FF"
           fontSize="16"
           lineHeight="24"
           marginLeft="24"
           marginRight="24"
           marginBottom="16" />
    <style id="ShiftItem"
           paddingLeft="24"
           paddingRight="24"
           flex="1"
           flexDirection="row"
           justifyContent="flex-start"
           marginBottom="32" />
    <style id="ShiftItem__Avatar"
           width="48"
           height="48"
           borderRadius="24" />
    <style id="ShiftItem__Info"
           marginLeft="16"
           flex="1"
           flexDirection="column" />
    <style id="ShiftItem__WorkerName"
           color="#4E4D4D"
           fontFamily="HKGrotesk-Regular"
           fontSize="16"
           lineHeight="24" />
    <style id="ShiftItem__Details"
           color="#8D9494"
           fontFamily="HKGrotesk-Regular"
           fontSize="16"
           lineHeight="24" />
    <style flex="1"
           flexDirection="row"
           id="Rating"
           margin="0" />
    <style height="24"
           id="Rating__Star"
           marginRight="8"
           width="24" />
    <style backgroundColor="#FBB90E"
           id="Rating__Star--Filled" />
  </styles>
  <body style="Body">
    <header style="Header">
      <image action="back"
             href="/index.xml"
             style="Header__Back"
             source="/case_studies/back@2x.png" />
    </header>
    <view style="Main"
          scroll="true">
      <view style="Section">
        <view style="Section__Top">
          <text style="Section__Header">Instaworkers</text>
          <text style="Section__Action">
          <behavior trigger="press"
                    action="intercom"
                    intercom:action="open" />Report Issue</text>
        </view>
        <list>
          <item key="a"
                action="new"
                href="/case_studies/rating_modal.xml"
                show-during-load="loadingScreen"
                delay="500"
                style="ShiftItem">
            <image source="/case_studies/image.jpg"
                   style="ShiftItem__Avatar" />
            <view style="ShiftItem__Info">
              <text style="ShiftItem__WorkerName">Jake Tepperman</text>
              <text style="ShiftItem__Details">7:00 PM - 1:00 AM - $116.80</text>
              <view style="Rating">
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=1"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=2"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=3"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=4"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=5"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
              </view>
            </view>
          </item>
          <item key="b"
                action="new"
                href="/case_studies/rating_modal.xml"
                show-during-load="loadingScreen"
                delay="500"
                style="ShiftItem">
            <image source="/case_studies/image.jpg"
                   style="ShiftItem__Avatar" />
            <view style="ShiftItem__Info">
              <text style="ShiftItem__WorkerName">Jake Tepperman</text>
              <text style="ShiftItem__Details">7:00 PM - 1:00 AM - $116.80</text>
              <view style="Rating">
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=1"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=2"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=3"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=4"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
                <image source="/case_studies/star_empty.png"
                       action="new"
                       href="/case_studies/rating_modal.xml?rating=5"
                       show-during-load="loadingScreen"
                       delay="500"
                       style="Rating__Star" />
              </view>
            </view>
          </item>
        </list>
      </view>
    </view>
  </body>
</screen>
<screen id="loadingScreen">
  <styles>
    <style alignItems="center"
           backgroundColor="white"
           borderBottomColor="#eee"
           borderBottomWidth="1"
           flexDirection="row"
           height="72"
           id="Header"
           paddingLeft="24"
           paddingRight="24"
           paddingTop="24" />
    <style width="32"
           height="32"
           id="Header__Close"
           marginRight="16" />
    <style backgroundColor="white"
           flex="1"
           id="Body" />
    <style color="black"
           fontFamily="HKGrotesk-Bold"
           lineHeight="40"
           fontSize="32"
           marginLeft="24"
           marginRight="24"
           marginTop="24"
           id="Title" />
    <style alignItems="center"
           flex="1"
           id="Loading"
           justifyContent="center" />
  </styles>
  <body style="Body">
    <header style="Header">
      <image action="close"
             source="/case_studies/close@2x.png"
             href="/index.xml"
             style="Header__Close" />
    </header>
    <text style="Title">Rate the Instaworker</text>
    <view style="Loading">
      <spinner />
    </view>
  </body>
</screen></doc>
